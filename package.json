{
  "name": "winter-arc-tracker",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:1p": "op run --env-file=.env.local -- npm run dev",
    "1p:setup": "echo 'Use op signin --account my.1password.com'",
    "build": "next build",
    "start": "next start",
    "dev:1p-prod": "op run --env-file=.env.1password.production -- npm run dev",
    "dev:1p-staging": "op run --env-file=.env.1password.staging -- npm run dev",
    "build:1p-prod": "op run --env-file=.env.1password.production -- npm run build",
    "build:1p-staging": "op run --env-file=.env.1password.staging -- npm run build",
    "preview": "npm run start",
    "lint": "npm run lint:code && npm run lint:secrets",
    "lint:code": "eslint . --max-warnings=0",
    "lint:secrets": "node scripts/check-secrets.mjs",
    "format": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:all": "npm run typecheck && npm run lint && npm run vercel:build && npm run test:unit",
    "vercel:build": "node scripts/run-vercel-build.mjs",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "e2e:ui": "playwright test --config e2e/playwright.config.ts --ui",
    "analyze": "ANALYZE=true npm run build",
    "perf:budget": "node scripts/check-budgets.mjs",
    "db:setup": "node scripts/setup-neon.mjs",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "lhci:run": "npm run build && npm run preview -- --port 4173 & sleep 3 && lhci autorun && kill $!",
    "agent:lighthouse": "node .agent/scripts/run-lighthouse.mjs",
    "agent:bundle": "node .agent/scripts/bundle-report.mjs",
    "agent:artifacts": "node .agent/scripts/prepare-artifacts.mjs",
    "clean": "node scripts/cleanup-repo.mjs",
    "clean:dry-run": "node scripts/cleanup-repo.mjs --dry-run",
    "check:consistency": "node scripts/consistency-check.mjs",
    "check:consistency:fix": "node scripts/consistency-check.mjs --fix",
    "docs:check": "node scripts/doc-hygiene.mjs",
    "docs:check:fix": "node scripts/doc-hygiene.mjs --fix",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@neondatabase/serverless": "^1.0.2",
    "@sentry/react": "10.18.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "eslint-config-next": "^16.0.1",
    "lucide-react": "^0.545.0",
    "next": "^16.0.1",
    "next-auth": "^5.0.0-beta.30",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "recharts": "^3.2.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@lhci/cli": "0.15.1",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.12",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "knip": "^5.64.2",
    "postcss": "^8.4.49",
    "react-swipeable": "^7.0.2",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vercel": "^48.10.2",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "wait-on": "^9.0.1"
  }
}
